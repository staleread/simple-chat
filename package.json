{
  "type": "module",
  "name": "simple-chat",
  "description": "Lightweight API example",
  "version": "2.0.0",
  "main": "server.js",
  "author": "Mykola Ratushniak",
  "license": "ISC",
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "start": "fastify start -l info src/server.js",
    "docker:up": "docker-compose --env-file .env.docker up -d",
    "docker:down": "docker-compose --env-file .env.docker down",
    "dev": "fastify start -w -l info -P src/server.js",
    "debug": "fastify start -w -d -l debug -P src/server.js",
    "lint": "biome check --write . && prisma format"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/env": "^5.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/redis": "^7.0.2",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.5.0",
    "@fastify/swagger-ui": "^5.2.2",
    "@fastify/view": "^11.1.0",
    "@fastify/websocket": "^11.0.2",
    "@prisma/client": "6.6.0",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.13",
    "eta": "^3.5.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "prisma": "^6.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4"
  }
}
